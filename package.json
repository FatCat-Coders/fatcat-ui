    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-styled-components-a11y": "^0.0.34",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "fast-memoize": "^2.5.2",
    "styled-components": "^5.3.0",
    "tslib": "^2.2.0"
  }
